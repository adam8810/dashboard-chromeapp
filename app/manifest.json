{
    "name": "Dashboard",
    "description": "Handy Dashboard",
    "version": "1",
    "manifest_version": 2,
    "permissions": [
        "https://maps.google.com/",

        "http://cf2.imgobject.com/",
        "http://image.tmdb.org/",
        "https://home.nest.com/",
        "http://api.themoviedb.org/",

        "alarms",
        "storage",
        "unlimitedStorage",
        "identity",
        "fullscreen"
    ],
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "app": {
        "background": {
            "scripts": [
                "scripts/runtime.js",
                "scripts/chromereload.js"
            ]
        }
    }

}